<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Semen Analysis</name>
    <description>Electro Ejaculation</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc" queryName="q_semenAnalysis"/>
            <destination schemaName="study" queryName="semenAnalysis" bulkLoad="true" batchSize="500" targetOption="truncate">
                <columnTransforms>
                    <column source="EEJ_DATE" target="date"/>
                </columnTransforms>
            </destination>
        </transform>
    </transforms>
    <schedule>
        <poll interval="60m"/>
    </schedule>
</etl>