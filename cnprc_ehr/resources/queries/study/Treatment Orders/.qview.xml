<customView xmlns="http://labkey.org/data/xml/queryCustomView">
   <columns>
       <column name="Id"/>
       <column name="date"/>
       <column name="enddate"/>
       <column name="category"/>
       <column name="frequency"/>
       <column name="project"/>
       <column name="code"/>
       <column name="drugName">
           <properties>
               <property name="columnTitle" value="Drug"/>
           </properties>
       </column>
       <column name="volume"/>
       <column name="vol_units"/>
       <column name="concentration"/>
       <column name="conc_units"/>
       <column name="amount"/>
       <column name="amount_units"/>
       <column name="route"/>
       <column name="weightOverride">
           <properties>
               <property name="columnTitle" value="Weight Override"/>
           </properties>
       </column>
       <column name="dosingNotes">
           <properties>
               <property name="columnTitle" value="Dosing Notes"/>
           </properties>
       </column>
       <column name="userComment">
           <properties>
               <property name="columnTitle" value="User Comment"/>
           </properties>
       </column>
   </columns>
   <sorts>
       <sort column="date" descending="true"/>
   </sorts>
</customView>