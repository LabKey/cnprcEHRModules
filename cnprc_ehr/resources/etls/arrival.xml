<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Arrivals</name>
    <description>Arrival data</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc" queryName="q_arrival" />
            <destination schemaName="study" queryName="arrival" bulkLoad="true" batchSize="500" targetOption="merge">
                <columnTransforms>
                    <column source="AN_ACQ_DATE" target="date"/>
                </columnTransforms>
            </destination>
        </transform>
    </transforms>
    <!--<incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="timestamp">-->
        <!--<deletedRowsSource schemaName="cnprcSrc" queryName="delete_arrival" timestampColumnName="audit_date_tm" deletedSourceKeyColumnName="OBJECT_ID" targetKeyColumnName="objectid"/>-->
    <!--</incrementalFilter>-->
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>