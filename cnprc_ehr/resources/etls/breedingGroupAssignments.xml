<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Breeding Group Assignments</name>
    <description>Breeding Group Assignments</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc" queryName="q_breedingGroupAssignments"/>
            <destination schemaName="study" queryName="breedingGroupAssignments" bulkLoad="true" batchSize="500" targetOption="merge">
                <columnTransforms>
                    <column source="ANBG_ASSIGNMENT_DATE" target="date"/>
                </columnTransforms>
            </destination>
        </transform>
    </transforms>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>