<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Biopsy</name>
    <description>Biopsies</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc" queryName="q_biopsy" />
            <destination schemaName="study" queryName="biopsy" bulkLoad="true" batchSize="500" targetOption="truncate">
                <columnTransforms>
                    <column source="PRM_WORK_PERFORMED_DATE" target="date"/>
                </columnTransforms>
            </destination>
        </transform>
    </transforms>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>