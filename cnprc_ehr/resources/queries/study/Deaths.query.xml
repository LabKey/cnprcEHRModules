<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="Cycle" tableDbType="TABLE" useColumnOrder="true">
                <columns>
                    <column columnName="Id"/>
                    <column columnName="date">
                        <columnTitle>Death Date</columnTitle>
                    </column>
                    <column columnName="cause">
                        <columnTitle>Cause</columnTitle>
                    </column>
                    <column columnName="necropsy">
                        <columnTitle>Necropsy</columnTitle>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=necropsy&amp;
                            query.id~eq=${id}
                        </url>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>necropsy</fkTable>
                            <fkColumnName>prmFk</fkColumnName>
                            <fkDisplayColumnName>reportType</fkDisplayColumnName>
                        </fk>
                    </column>
                    <column columnName="manner">
                        <columnTitle>Death Manner</columnTitle>
                            <fk>
                                <fkDbSchema>ehr_lookups</fkDbSchema>
                                <fkTable>death_manner</fkTable>
                                <fkColumnName>value</fkColumnName>
                                <fkDisplayColumnName>Title</fkDisplayColumnName>
                            </fk>
                    </column>
                    <column columnName="dam">
                        <columnTitle>Dam</columnTitle>
                    </column>
                    <column columnName="notAtCenter">
                        <columnTitle>Not at Center</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>