<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="LocationReport" tableDbType="NOT_IN_DB" useColumnOrder="true">
                <columns>
                    <column columnName="Id">
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>animal</fkTable>
                            <fkColumnName>Id</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="location">
                        <url>/query/executeQuery.view?schemaName=study&amp;
                        query.queryName=LocationReport&amp;
                        query.location~eq=${location}
                        </url>
                    </column>
                    <column columnName="Id/demographicsActivePregnancy/conNum">
                        <columnTitle>Pregnancy</columnTitle>
                        <!--<url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=DemographicsActivePregnancy&amp;
                            query.id~eq=${Id}
                        </url>-->
                    </column>
                    <column columnName="room">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="area"/>
                </columns>
            </table>
        </tables>
    </metadata>
</query>