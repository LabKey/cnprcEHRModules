<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Census Flag</name>
    <description>Animal census flag assignments</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc" queryName="flags" />
            <destination schemaName="study" queryName="flags" bulkLoad="true" targetOption="truncate" >
                <columnTransforms>
                    <column source="ANCENS_FLAG_ASSIGNED" target="date"/>
                </columnTransforms>
            </destination>
        </transform>
    </transforms>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>
