<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Images</name>
    <description>SNOMED and Pathology images</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc_image" queryName="q_image" />
            <destination schemaName="cnprc_ehr" queryName="image" bulkLoad="true" batchSize="500" targetOption="truncate"/>
        </transform>
    </transforms>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>