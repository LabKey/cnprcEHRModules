<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Gross Findings</name>
    <description>Biopsy observations per organ per report</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc" queryName="q_grossFindings" />
            <destination schemaName="study" queryName="grossFindings" bulkLoad="true" batchSize="500" targetOption="truncate">
                <columnTransforms>
                    <column source="PR_DATE" target="date"/>
                </columnTransforms>
            </destination>
        </transform>
    </transforms>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>