<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
<name>Weights</name>
<description>Animal weights</description>
<transforms>
    <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
        <description>Copy to target</description>
        <source schemaName="cnprcSrc" queryName="weight" />
        <destination schemaName="study" queryName="weight" bulkLoad="true" batchSize="5000" targetOption="truncate">
            <columnTransforms>
                <column source="WT_DATE" target="date"/>
            </columnTransforms>
        </destination>
    </transform>
</transforms>
    <!--<incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="timestamp">-->
        <!--<deletedRowsSource schemaName="cnprcSrc" queryName="delete_weight" timestampColumnName="audit_date_tm" deletedSourceKeyColumnName="OBJECT_ID" targetKeyColumnName="objectid"/>-->
    <!--</incrementalFilter>-->
<schedule>
    <poll interval="60m" />
</schedule>
</etl>
