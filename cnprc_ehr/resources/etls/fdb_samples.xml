<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>FreezerDB Samples</name>
    <description>Freezer sample database</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc_fdb" queryName="q_fdb_samples"/>
            <destination schemaName="study" queryName="fdb_samples" bulkLoad="true" targetOption="truncate">
                <columnTransforms>
                    <column source="FDS_COLLECTION_DATE" target="date"/>
                </columnTransforms>
            </destination>
        </transform>
        <transform id="step2" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc_fdb" queryName="q_fdb_samples_no_id"/>
            <destination schemaName="cnprc_ehr" queryName="fdb_samples_no_id" bulkLoad="true" targetOption="truncate"/>
        </transform>
    </transforms>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>