<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="flags" tableDbType="TABLE" useColumnOrder="true">
                <columns>
                    <column columnName="Id"/>
                    <column columnName="flag">
                        <columnTitle>Flag</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>flag_codes</fkTable>
                            <fkColumnName>value</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="remark">
                        <columnTitle>Explanation</columnTitle>
                    </column>
                    <column columnName="date">
                        <columnTitle>Assign Date</columnTitle>
                        <formatString>Date</formatString>
                    </column>
                    <column columnName="enddate">
                        <isHidden>true</isHidden>
                        <shownInInsertView>true</shownInInsertView>
                        <shownInDetailsView>true</shownInDetailsView>
                        <columnTitle>Removed Date</columnTitle>
                        <formatString>Date</formatString>
                    </column>
                    <column columnName="projectCode">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="performedby">
                        <isHidden>true</isHidden>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>


