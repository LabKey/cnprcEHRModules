        <column name="rowId"/>
        <column name="rowPk"/>
        <column name="source"/>
        <column name="fileLineNumber"/>
        <column name="status"/>
        <column name="voided"/>
        <column name="data"/>
        <column name="observationType"/>
        <column name="transferredToMhObs"/>
