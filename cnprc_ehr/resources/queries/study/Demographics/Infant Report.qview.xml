<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="Id/curLocation">
            <properties>
                <property name="columnTitle" value="Location"/>
            </properties>
        </column>
        <column name="Id/AssignmentCurrent/projectCode"/>
        <column name="gender"/>
        <column name="Id/age/yearsAndMonths">
            <properties>
                <property name="columnTitle" value="Age today Y:M"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/MostRecentWeight">
            <properties>
                <property name="columnTitle" value="Current Weight (kg)"/>
            </properties>
        </column>
        <column name="Id/attributes/flag"/>
        <column name="dam"/>
        <column name="sire"/>
    </columns>
    <filters>
        <filter column="Id/age/ageInDays" operator="lt" value="366"/>
    </filters>
    <sorts>
        <sort column="Id" descending="false"/>
    </sorts>
</customView>