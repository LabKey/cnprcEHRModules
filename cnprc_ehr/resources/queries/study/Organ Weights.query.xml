<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="Organ Weights" tableDbType="TABLE" useColumnOrder="true">
                <columns>
                    <column columnName="Id"/>
                    <column columnName="date">
                        <columnTitle>Work Performed Date</columnTitle>
                    </column>
                    <column columnName="enddate">
                        <columnTitle>Report Completion Date</columnTitle>
                    </column>
                    <column columnName="tissue">
                        <columnTitle>Organ Measurement Type</columnTitle>
                        <!--<fk>-->
                            <!--<fkDbSchema>ehr_lookups</fkDbSchema>-->
                            <!--<fkTable>necropsy_measurement_types</fkTable>-->
                            <!--<fkColumnName>value</fkColumnName>-->
                        <!--</fk>-->
                    </column>
                    <column columnName="measurementValue">
                        <columnTitle>Measurement</columnTitle>
                    </column>
                    <column columnName="unit">
                        <columnTitle>Unit</columnTitle>
                    </column>
                    <column columnName="projectCode">
                        <columnTitle>Project Code</columnTitle>
                        <fk>
                            <fkDbSchema>ehr</fkDbSchema>
                            <fkTable>project</fkTable>
                            <fkColumnName>name</fkColumnName>
