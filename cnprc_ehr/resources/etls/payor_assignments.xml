<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Payor Assignments</name>
    <description>Per diem Payor Assignment History</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc" queryName="q_payor_assignments"/>
            <destination schemaName="study" queryName="payor_assignments" bulkLoad="true" targetOption="truncate">
                <columnTransforms>
                    <column source="ANPAY_ASSIGNMENT_DATE" target="date"/>
                </columnTransforms>
            </destination>
        </transform>
    </transforms>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>
