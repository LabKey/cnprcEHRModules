<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Pathology Diagnoses</name>
    <description>SNOMED records for Animals and Conceptions/Fetuses</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc" queryName="q_pathology_diagnoses"/>
            <destination schemaName="study" queryName="pathologyDiagnoses" bulkLoad="true" batchSize="500" targetOption="truncate">
                <columnTransforms>
                    <column source="SNO_DATE" target="date"/>
                </columnTransforms>
            </destination>
        </transform>
    </transforms>
    <schedule>
        <poll interval="60m"/>
    </schedule>
</etl>