<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="pregnancyConfirmation" tableDbType="TABLE" useColumnOrder="true">
                <columns>
                    <column columnName="Id"/>
                    <column columnName="date">
                        <columnTitle>Sample Date</columnTitle>
                    </column>
                    <column columnName="method">
                        <columnTitle>Determination Method</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>preg_det_method</fkTable>
                            <fkColumnName>value</fkColumnName>
                            <fkDisplayColumnName>Description</fkDisplayColumnName>
                        </fk>
                    </column>
                    <column columnName="result">
                        <columnTitle>Determination Result</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>preg_det_result</fkTable>
                            <fkColumnName>value</fkColumnName>
                            <fkDisplayColumnName>Description</fkDisplayColumnName>
                        </fk>
                    </column>
                    <column columnName="scheduleStatus">
                        <columnTitle>Schedule Status</columnTitle>
                    </column>
                    <column columnName="cycleDay1">
                        <columnTitle>Cycle Start Date</columnTitle>
                    </column>
                    <column columnName="termDate">
                        <columnTitle>Term Date</columnTitle>
                    </column>
                    <column columnName="termCode">
                        <columnTitle>Term Code</columnTitle>
                    </column>
                    <column columnName="sire">
                        <columnTitle>Sire</columnTitle>
                        <url>/ehr/participantView.view?participantId=${sire}</url>
                    </column>
                    <column columnName="confirmDate">
                        <columnTitle>Confirm Date</columnTitle>
                    </column>
                    <column columnName="lastObscanDate">
                        <columnTitle>Last Observation Scan Date</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>